        rdfs:label          "category" ;
        rdfs:domain         vf:EconomicResource ;
        rdfs:range          vf:ResourceTaxonomyItem ;
        rdfs:comment        "A type or categorization that describes a resource, at any level of granularity." .

vf:committedCategory
