        rdfs:range          vf:Process .

vf:committedInputOf
        a                   owl:ObjectProperty ;
        rdfs:label         "committed input of" ;
        rdfs:domain         vf:Commitment ;
        rdfs:range          vf:Process .

vf:committedOutputOf
        a                   owl:ObjectProperty ;
        rdfs:label         "committed output of" ;
        rdfs:domain         vf:Commitment ;
        rdfs:range          vf:Process .

